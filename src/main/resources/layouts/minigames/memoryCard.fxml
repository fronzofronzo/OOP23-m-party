<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/15" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unibo.mparty.view.minigames.memorycard.MemoryCardViewImpl">
   <FlowPane fx:id="cardsPane" alignment="CENTER" columnHalignment="CENTER" hgap="20.0" maxHeight="400.0" maxWidth="450.0" prefHeight="368.0" prefWidth="369.0" vgap="20.0">
      <VBox.margin>
         <Insets bottom="20.0" left="300.0" right="300.0" top="20.0" />
      </VBox.margin>
   </FlowPane>
   <HBox alignment="CENTER" nodeOrientation="RIGHT_TO_LEFT" prefHeight="200.0" prefWidth="200.0">
      <children>
         <Button fx:id="controlButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#startGame" prefHeight="93.0" prefWidth="209.0" text="Start !" wrapText="true">
            <font>
               <Font size="18.0" />
            </font>
         </Button>
         <Label fx:id="textLabel" lineSpacing="2.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="209.0" prefWidth="724.0" text="Label" textAlignment="JUSTIFY" wrapText="true">
            <font>
               <Font size="15.0" />
            </font>
            <HBox.margin>
               <Insets left="20.0" />
            </HBox.margin>
         </Label>
      </children>
      <padding>
         <Insets bottom="10.0" left="20.0" top="10.0" />
      </padding>
   </HBox>
</VBox>
