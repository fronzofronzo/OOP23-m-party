<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unibo.mparty.view.minigames.memoryCard.MemoryCardViewImpl">
   <children>
      <FlowPane fx:id="cardsPane" alignment="CENTER" columnHalignment="CENTER" hgap="20.0" maxHeight="400.0" maxWidth="450.0" prefHeight="368.0" prefWidth="369.0" vgap="20.0">
         <VBox.margin>
            <Insets bottom="20.0" left="300.0" right="300.0" top="20.0" />
         </VBox.margin>
      </FlowPane>
      <Button fx:id="controlButton" mnemonicParsing="false" onAction="#startGame" prefHeight="93.0" prefWidth="209.0" text="Start !">
         <font>
            <Font name="Segoe UI Light" size="18.0" />
         </font>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Button>
      <Label fx:id="textLabel" alignment="CENTER" prefHeight="211.0" prefWidth="351.0" textAlignment="JUSTIFY" wrapText="true">
         <font>
            <Font name="Segoe UI Light" size="17.0" />
         </font>
      </Label>
   </children>
</VBox>
